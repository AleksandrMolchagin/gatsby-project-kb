{
  "name": "gatsby-project-kb",
  "private": true,
  "devDependencies": {
    "@types/react": "^16.9.43",
    "@types/react-dom": "^16.9.8",
    "gatsby": "^2.28.0",
    "lerna": "^3.22.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "tsdx": "^0.14.1",
    "typescript": "^4.1.2"
  },
  "workspaces": [
    "packages/*",
    "demo"
  ],
  "scripts": {
    "lerna": "lerna",
    "dev": "yarn workspace demo develop",
    "demo:build": "NODE_ENV=production yarn workspace demo build",
    "test": "lerna run test --",
    "lint": "lerna run lint -- --fix",
    "build": "yarn demo:build",
    "build-gh-pages": "KB_BASE_PATH='/gatsby-project-kb' yarn demo:build",
    "prepublish": "lerna run prepublish",
    "start:app": "yarn run build && yarn --cwd example && yarn --cwd example start"
  }
}
